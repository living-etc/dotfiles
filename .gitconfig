[alias]
  lg = log --graph --decorate --pretty=format:'%C(auto)%h%Creset -%C(auto)%d%Creset %s %Cgreen(%ar) %C(bold blue)<%an>%Creset'
[core]
  editor = vim
  excludesfile = ~/.gitignore
[push]
  default = nothing
[commit]
  template = ~/.gitmessage
